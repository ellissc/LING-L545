DELIMITERS = "." ;

LIST DET = DET ;
LIST PUNCT = PUNCT ;
LIST ADP = ADP ;
LIST NOUN = NOUN ;
LIST PRON = PRON ;
LIST Nom = Nom ;
LIST VERB = VERB ;
LIST ADV = ADV ;
LIST CCONJ = CCONJ ;

SECTION

REMOVE DET IF (1C PUNCT) ;
REMOVE DET IF (NOT 1C PRON OR NOUN) ;
REMOVE NOUN IF (1C NOUN OR PRON) ;
SELECT CCONJ IF (-1 ADV OR VERB) (1C ADV OR VERB) ;
